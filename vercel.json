{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_assets/(.*)",
      "dest": "/public/_assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/_assets/(.*)",
      "destination": "/_assets/$1"
    }
  ]
}
